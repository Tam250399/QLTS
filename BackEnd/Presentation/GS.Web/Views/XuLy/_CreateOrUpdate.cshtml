                    }
                    console.log(_data);
                    $.ajax({
                        type: "POST",
                        data: _data,
                        url: "/XuLy/ViewThuocTinhByHinhThuc",
                        success: function (data) {
                            if (CheckValidMessageReturn(data)) {
