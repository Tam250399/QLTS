        <label class="col-form-label col-sm-2">Số máy</label>
        <div class="col-sm-4">
            <nop-editor asp-for="taisanOtoModel.SO_MAY" />
            <span asp-validation-for="taisanOtoModel.SO_MAY" data-valmsg-for="taisanOtoModel.SO_MAY"></span>
