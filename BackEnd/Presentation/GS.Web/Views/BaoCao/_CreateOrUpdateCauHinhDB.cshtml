@model CauHinhBaoCaoDBModel
<div class="form-group row">
        <label class="col-form-label col-sm-2 text-sm-left">Mã báo cáo Đồng bộ</label>
        <div class="col-sm-4">
            <nop-editor asp-for="MaBaoCaoKho" />
        </div>
        <label class="col-form-label col-sm-2 text-sm-left">Model</label>
        <div class="col-sm-4">
            <nop-editor asp-for="Model" />
        </div>
</div>
<div class="form-group row">
    <label class="col-form-label col-sm-2 text-sm-left">Report class</label>
    <div class="col-sm-4">
        <nop-editor asp-for="FullName_ReportClass" />
    </div>
    <label class="col-form-label col-sm-2 text-sm-left">Search model</label>
    <div class="col-sm-4">
        <nop-editor asp-for="FullName_SearchModelClass" />
    </div>
</div>
<div class="form-group row">
    <label class="col-form-label col-sm-2 text-sm-left">ExcecuteStatment</label>
    <div class="col-sm-10">
        <nop-textarea asp-for="ExcecuteStatment" />
    </div>
</div>

<div class="gs-form-group ">
    <h6>
        Dữ liệu đầu vào
    </h6>
    @foreach (var a in Model.Lstbody)
    {
        <div class="form-group row" >
            <label class="col-form-label col-sm-2 text-sm-left" >@a.Key</label>
            <div class="col-sm-10 cau-hinh-bc-input">
                <nop-editor asp-for="@a.Value" id="@a.Key" />
            </div>
        </div>
        <script>
        </script>
    }
</div>
<div class="gs-form-group ">
        <h6>
            Dữ liệu đầu ra
        </h6>
        @foreach (var b in Model.LstreportData)
        {
            <div class="form-group row ">
                <label class="col-form-label col-sm-2 text-sm-left">@b.Key</label>
                <div class="col-sm-10 cau-hinh-bc-output">
                    <nop-editor asp-for="@b.Value" id="@b.Key" />
                </div>
            </div>
        }
    </div>
