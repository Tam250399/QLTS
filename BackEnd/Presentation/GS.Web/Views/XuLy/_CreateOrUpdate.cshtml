                        QUYET_DINH_NGAY: $("#@Html.IdFor(c=>c.QUYET_DINH_NGAY)").val(),
                        LoaiXuLy: $("#@Html.IdFor(c=>c.LOAI_XU_LY_ID)").val(),
                        LOAI_XU_LY_ID: $("#LOAI_XU_LY_ID").val(),
