                <span asp-validation-for="taisanOtoModel.BIEN_KIEM_SOAT"></span>
            </div>
        </div>
        <label class="col-form-label col-sm-2 text-required d-none">Tên tài sản</label>
        <div class="col-sm-4 d-none">
            <nop-editor asp-for="TEN" />
        </div>
