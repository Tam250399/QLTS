        <Item11 Ref="24" ControlType="XRLabel" Name="label18" Multiline="true" Text="VI. Kiến nghị các giải pháp nhằm hoàn thiện hệ thống pháp luật, nâng cao hiệu quả công tác quản lý, sử dụng tài sản nhà nước:" TextAlignment="TopLeft" SizeF="752,43.020874" LocationFloat="0, 782.465942" Font="Times New Roman, 12pt, style=Bold" Padding="2,2,0,0,100">
