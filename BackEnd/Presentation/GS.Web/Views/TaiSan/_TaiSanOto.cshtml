                if ($('#SelectedChucDanhIds[disabled]').get(0)) {
                    $('#span_arrow_listChucDanh').css("color", "#6565658c");

                } else {
                    $('#span_arrow_listChucDanh').css("color", "#000000");
                }
			});
			ListchucDanh.trigger('change');
		}

		if (chucDanh) {
			chucDanh.bind('change', ()=>
