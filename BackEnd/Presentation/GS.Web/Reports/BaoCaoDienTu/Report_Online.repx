        <Item15 Ref="32" ControlType="XRLabel" Name="label14" Multiline="true" Text="IV. Tình hình thực hiện công khai quản lý, sử dụng tài sản nhà nước:" TextAlignment="TopLeft" SizeF="752.000061,28.437561" LocationFloat="0, 678.0905" Font="Times New Roman, 12pt, style=Bold" Padding="2,2,0,0,100">
