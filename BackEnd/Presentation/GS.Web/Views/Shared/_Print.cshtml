@using GS.Core
@inject IWorkContext workContext;
@{
    var themeName = "AppWork";
}
<!DOCTYPE html>
<html lang="en" @Html.GSPageCssClasses()>
<head>
    <title>@Html.GSTitle()</title>
    <meta http-equiv="Content-type" content="text/html;charset=UTF-8" />
    <link href="@Url.Content($"~/Themes/{themeName}/Content/css/print.css")" rel="stylesheet" type="text/css" />
    <script src="@Url.Content("~/lib/jquery/jquery-3.3.1.min.js")"></script>
    <script src="@Url.Content("~/lib/jquery-migrate/jquery-migrate-3.0.1.min.js")"></script>
    @Html.GSInlineScripts(this.Url, ResourceLocation.Head)
</head>
<body>
    @RenderBody()
    @Html.GSInlineScripts(this.Url, ResourceLocation.Footer)
</body>
</html>
